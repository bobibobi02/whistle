{
  "expo": {
    "name": "Whistle",
    "slug": "whistle",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "scheme": "whistleapp",
    "entryPoint": "./AppEntry.js",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.bobi02.whistle",
      "permissions": ["NOTIFICATIONS"]
    },
    "ios": {
      "bundleIdentifier": "com.bobi02.whistle",
      "supportsTablet": true,
      "infoPlist": {
        "NSNotificationsUsageDescription": "This app uses push notifications to keep you updated.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "extra": {
      "apiUrl": "https://your-domain.com",
      "eas": {
        "projectId": "1c9b1e72-7b52-4f51-81f2-0567af3dd9b7"
      }
    },
    "plugins": [
      "expo-font",
      "expo-notifications"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "experiments": {
      "typedRoutes": false
    }
  }
}
